        <statetype name="videodescrip">
            <area>100%-230,70,110,32</area>
            <showempty>yes</showempty>
            <state name="SD">
                <imagetype name="SD">
                    <area>0,0,110,32</area>
                    <filename>images/icons/VideoSD.png</filename>
                </imagetype>
            </state>
            <state name="HD_720_I">
                <imagetype name="720i">
                    <area>0,0,110,32</area>
                    <filename>images/icons/VideoHD720.png</filename>
                </imagetype>
            </state>
            <state name="HD_720_P">
                <imagetype name="720p">
                    <area>0,0,110,32</area>
