<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mythuitheme SYSTEM "http://www.mythtv.org/schema/mythuitheme.dtd">
<mythuitheme>

    <window name="programguide">
        <group name="decoration" from="basewindowdecoration">
            <clock name="date">
                <alpha>0</alpha>
            </clock>
        </group>
        <textarea name="pagetitle" from="basetitletext">
            <value>Program Guide</value>
        </textarea>
        <textarea name="datetext" from="basetext_s">
            <area>25,0,150,50</area>
            <align>hcenter,vcenter</align>
            <alpha>200</alpha>
        </textarea>
        <group name="epg">
            <area>25,65,1866,650</area>
            <shape name="background" from="basecontrolbackground">
                <area>0,0,148,100%</area>
            </shape>
            <buttonlist name="timelist">
                <area>0,0,150,100%</area>
                <buttonarea>0,0,100%,100%</buttonarea>
                <layout>vertical</layout>
                <arrange>stack</arrange>
                <statetype name="buttonitem">
                    <area>0,0,100%,99</area>
                    <state name="active">
                        <area>0,0,100%,99</area>
                        <textarea name="buttontext" from="basetext_s">
                            <area>10,10,-10,100%</area>
                            <align>center</align>
                            <alpha>100</alpha>
                        </textarea>
                    </state>
                    <state name="selectedactive" from="active"/>
                    <state name="inactive" from="active"/>
                    <state name="selectedinactive" from="selectedactive"/>
                </statetype>
            </buttonlist>
            <buttonlist name="channellist">
                <area>150,0,100%,50</area>
                <buttonarea>0,0,100%,100%</buttonarea>
                <layout>horizontal</layout>
                <arrange>stack</arrange>
                <statetype name="buttonitem">
                    <area>0,0,286,100%</area>
                    <state name="active">
                        <area>0,0,286,100%</area>
                        <shape name="background" from="basecontrolbackground">
                            <area>-1,-1,100%-1,100%-1</area>
                            <line color="#000000" width="1"/>
                        </shape>
                        <imagetype name="channelicon">
                            <area>10,50%-16,32,32</area>
                            <alpha>200</alpha>
                        </imagetype>
                        <imagetype name="sender" depends="!channelicon">
                            <area>10,50%-16,32,32</area>
                            <filename>images/icons/Sender.png</filename>
                            <alpha>100</alpha>
                        </imagetype>
                        <textarea name="channelnumber" from="basetext_s">
                            <area>0,0,100%,100%</area>
                            <align>center</align>
                            <alpha>100</alpha>
                        </textarea>
                    </state>
                    <state name="selectedactive" from="active"/>
                    <state name="inactive" from="active"/>
                    <state name="selectedinactive" from="selectedactive"/>
                </statetype>
            </buttonlist>
            <shape name="gridbackground">
                <area>150,50,100%,100%</area>
                <fill color="#FFFFFF" alpha="30"/>
            </shape>
            <guidegrid name="guidegrid">
                <!-- BUG: placement of guid grid is not relative to parent -->
                <area>175,115,1716,600</area>
                <layout>vertical</layout>
                <font>basefont_xs</font>
                <channels>6</channels>
                <timeslots>6</timeslots>
                <textoffset>10,5</textoffset>
                <showcategorycolors>no</showcategorycolors>
                <solidcolor>#323232</solidcolor>
                <recordingcolor>#326432</recordingcolor>
                <conflictingcolor>#643232</conflictingcolor>
                <selector type="box" fillcolor="#646464" ></selector>
                <recordstatus type="SingleRecord" image="images/icons/Record.png"></recordstatus>
                <recordstatus type="TimeslotRecord" image="images/icons/Clock.png"></recordstatus>
                <recordstatus type="ChannelRecord" image="images/icons/Sender.png"></recordstatus>
                <recordstatus type="AllRecord" image="images/icons/All.png"></recordstatus>
                <recordstatus type="WeekslotRecord" image="images/icons/Calendar.png"></recordstatus>
                <recordstatus type="FindOneRecord" image="images/icons/Find.png"></recordstatus>
                <recordstatus type="OverrideRecord" image="images/icons/Override.png"></recordstatus>
            </guidegrid>
            <!-- hide misalignment of right side of epg -->
            <shape name="rightcut">
                <area>100%-5,0,5,100%</area>
                <fill color="#000000"/>
            </shape>
        </group>
        <group name="details">
            <area>25,750,-25,-75</area>
            <group name="channelimage">
                <area>0,0,200,250</area>
                <shape name="iconbackground">
                    <area>0,0,100%,100%</area>
                    <type>roundbox</type>
                    <cornerradius>5</cornerradius>
                    <fill color="#FFFFFF" alpha="25" />
                    <line color="#000000" alpha="0" width="0" />
                </shape>
                <imagetype name="channelicon">
                    <area>0,0,100%,100%-50</area>
                </imagetype>
                <imagetype name="sendericon" depends="!channelicon">
                    <area>25,25,-25,-75</area>
                    <filename>images/icons/SenderBig.png</filename>
                    <alpha>100</alpha>
                </imagetype>
                <textarea name="channel" from="basetext_s">
                    <area>0,100%-50,100%,50</area>
                    <align>center</align>
                    <alpha>200</alpha>
                </textarea>
            </group>
            <textarea name="title" from="basetext_m">
                <area>250,0,-1,50</area>
                <align>left,top</align>
                <template>%TITLE| %%~ |00x00| %%~ |SUBTITLE%</template>
            </textarea>
            <textarea name="timedate" from="basetext_s">
                <area>250,50,-1,40</area>
                <template>%TIMEDATE% (%LENMINS%)</template>
            </textarea>
            <textarea name="description" from="basetext_s">
                <area>250,100,-1,100%</area>
                <multiline>yes</multiline>
                <cutdown>yes</cutdown>
                <align>justify,top</align>
                <alpha>200</alpha>
            </textarea>
        </group>
        <group name="footer">
            <area>25,100%-50,-25,50</area>
            <textarea name="channelgroup" from="basetext_s">
                <area>0,0,100%,100%</area>
                <align>center</align>
                <alpha>150</alpha>
            </textarea>
        </group>
    </window>

