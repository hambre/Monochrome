    <window name="manualschedule">
        <group name="decoration" from="basesettingsdecoration"/>
        <textarea name="pagetitle" from="basetitletext">
            <value>Manual Recording Schedule</value>
        </textarea>
        <group name="widgets">
            <area>50%-500,50%-250,1000,300</area>
            <group name="labels">
                <area>0,0,35%,100%</area>
                <textarea name="titlelabel" from="basetext_s">
                    <area>0,0,100%,40</area>
                    <value>Title (Optional):</value>
                </textarea>
                <textarea name="channellabel" from="basetext_s">
                    <area>0,65,100%,40</area>
                    <value>Channel:</value>
                </textarea>
                <textarea name="datelabel" from="basetext_s">
                    <area>0,130,100%,40</area>
                    <value>Date, or day of week:</value>
                </textarea>
                <textarea name="timelabel" from="basetext_s">
                    <area>0,195,100%,40</area>
                    <value>Start Time:</value>
                </textarea>
                <textarea name="durationlabel" from="basetext_s">
                    <area>0,260,100%,40</area>
                    <value>Duration:</value>
                </textarea>
            </group>
            <group name="controls">
                <area>35%,0,100%,100%</area>
                <textedit name="title" from="basetextedit">
                    <area>0,0,100%,40</area>
                </textedit>
                <buttonlist name="channel" from="baseselector">
                    <area>0,65,100%,40</area>
                </buttonlist>
                <buttonlist name="startdate" from="baseselector">
                    <area>0,130,100%,40</area>
                </buttonlist>
                <spinbox name="starthour" from="basespinbox">
                    <area>0,195,100,40</area>
                </spinbox>
                <textarea name="timesep" from="basetext_s">
                    <area>110,195,5,40</area>
                    <value>:</value>
                </textarea>
                <spinbox name="startminute" from="basespinbox">
                    <area>125,195,100,40</area>
                </spinbox>
                <spinbox name="duration" from="basespinbox">
                    <area>0,260,100,40</area>
                </spinbox>
            </group>
        </group>
        <!-- bottom part with buttons -->
        <button name="next" from="basebutton">
            <area>50%-225,100%-65,200,40</area>
            <value>Next</value>
        </button>
        <button name="cancel" from="basebutton">
            <area>50%+25,100%-65,200,40</area>
            <value>Cancel</value>
        </button>
    </window>

